    order: workflow
    list:
    - group:
      - methodname: Database
        methodtype: method
        outputname: getPreviousData
        outputtype: function
    - group:
      - methodname: AttributePosition
        methodtype: method
        outputname: getPreviousPosition
        outputtype: function
      - methodname: AttributeForce
        methodtype: method
        outputname: getPreviousForce
        outputtype: function
    - group:
      - methodname: steepestDescent
        methodtype: method
        outputname: getCurrentPosition
