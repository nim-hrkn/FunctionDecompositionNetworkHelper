  block:

  - blockname:  hyperparemter opt bruteforce
    order: workflow
    list:
    - group:
      - methodname: PreviousBestHyperparameterScoreSet
        methodtype: method
        outputname: getPreviousBestHyperparameterScore
        outputtype: function
    - group:
      - methodname: AttributeHyperparameter
        methodtype: method
        outputname: getAttributeHyperparameter
        outputtype: function
    - group:
      - methodname: AttributeHyperparameterSetting
        methodtype: method
        outputname: getHyperparameterCandidates
        outputtype: function
      - methodname: AttributeHyperparameterSetting
        methodtype: method
        outputname: getHyperparameterCandidates
        outputtype: function
    - group:
      - methodname: RegressionScores
        methodtype: method
        outputname: getRegressionScores
        outputtype: function
    - group:
      - methodname: chooseTheBestScoreModel
        methodtype: function
        outputname: chooseTheBestHyperparameterAndScoreWithinHyperparameterSetting
        outputtype: function


  - blockname:  hyperparemter opt bruteforce
    order: workflow
    list:
    - group:
      - outputname: chooseTheBestHyperparameterAndScoreWithinHyperparameterSetting
        outputtype: function
    - group:
      - methodname: CurrentBest!=PreviousBest
        methodtype: function
        outputname: getNewHyperparameter
