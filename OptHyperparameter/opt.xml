  - blockname:  hyperparemter opt bruteforce
    order: workflow
    list:
    - group:
      - outputname: chooseTheBestHyperparameterAndScoreWithinHyperparameterSetting
        outputtype: function
      - outputname: chooseThePreviousBestScore
        outputtype: function
    - group:
      - methodname: CurrentBest==PreviousBest
        methodtype: function
        outputname: getTheBestHyperparameter
        outputtype: function


  - blockname:  hyperparemter opt bruteforce
    order: workflow
    list:
    - group:
      - outputname: getPreviousBestHyperparameterScore
        outputtype: function
    - group:
      - methodname: AttributeScore
        methodtype: method
        outputname: chooseThePreviousBestScore
        outputtype: function
    - group:
      - methodname: CurrentBest!=PreviousBest
        methodtype: function
        outputname: getNewHyperparameter
        outputtype: function


  - blockname:  hyperparemter opt bruteforce
    order: workflow
    list:
    - group:
      - outputname: getNewHyperparameter
        outputtype: function
      - methodname: PreviousBestHyperparameterScoreSet
        methodtype: method
        outputname: getPreviousBestHyperparameterScoreSet
        outputtype: function
    - group:
      - methodname: updateHyperparameter
        methodtype: function
        outputname: findBetterHyperparameter
